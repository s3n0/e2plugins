msgstr "Speichern & Beenden"

msgid "Update plugin"
msgstr "Update-Plugin"

msgid "Update picons"
msgstr "Picon herunterladen"

msgid "Picon folder"
msgstr "Ordner für Picons"

msgid "User defined folder"
msgstr "Benutzerdefinierter Ordner"

msgid "Picon update method"
msgstr "Picon-Aktualisierungsmethode"

msgid "copy all: current will deleted"
msgstr "kopieren Sie alle: Aktuell wird gelöscht"

msgid "copy all: incremental update"
msgstr "kopieren Sie alle: inkrementelle Aktualisierung"

msgid "sync with TV userbouquets"
msgstr "synchronisieren mit TV-Kanallisten"

msgid "sync with TV+RADIO userbouquets"
msgstr "synchronisieren mit TV+RADIO-Kanallisten"

msgid "Satellite positions"
msgstr "Satellitenposition"

msgid "Select satellites:"
msgstr "Auswahl der Satelliten:"

msgid "Picon resolution"
msgstr "Picon Größe"

msgid "Picon background"
msgstr "Hintergrund picon"

